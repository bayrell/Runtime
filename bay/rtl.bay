	}
	
	
	/**
	 * Set global context
	 * @param Context context
	 */
	static void setContext(Context context)
	{
		#switch
		#case ifcode PHP then
		static::$_global_context = $context;
		#case ifcode JAVASCRIPT then
		use("Runtime.rtl")._global_context = context;
		#case ifcode ES6 then
